              "type": "string"
            },
            {
              "id": "total-accounts",
              "name": "total_accounts",
              "value": "={{ $json.summary.total_accounts_analyzed }}",
              "type": "number"
            },
            {
              "id": "flagged-count",
              "name": "flagged_count",
              "value": "={{ $json.summary.suspicious_accounts_flagged }}",
              "type": "number"
            },
            {
              "id": "rings-count",
              "name": "rings_detected",
              "value": "={{ $json.summary.fraud_rings_detected }}",
              "type": "number"
            }
          ]
        },
        "options": {}
      },
      "id": "set-alert",
      "name": "Build Alert",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3.2,
      "position": [880, 200]
    },
    {
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "id": "clean-msg",
              "name": "message",
              "value": "âœ… Clean â€” No suspicious accounts detected in {{ $json.summary.total_accounts_analyzed }} accounts analyzed.",
